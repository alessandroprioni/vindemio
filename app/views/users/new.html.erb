<% provide(:title, 'Sign up') %>
<div class="row">
	<div class="large-12 columns center">
		<h1>Sign up</h1>
	</div>
</div>

<div class="row">
	<div class="large-12 columns">
		<%= form_for(@user) do |f| %>
	  	<%= render 'fields', f: f %>
      <%= f.submit "Create my account", class: "button" %>
    <% end %>
	</div>
</div>